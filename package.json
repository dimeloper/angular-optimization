    "eslint": "^9.25.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.2.5",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.6.3",
    "vite": "^6.3.3",
